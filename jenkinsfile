pipeline {
    agent {
        label "windows"
    }
	tools {
	    maven '3.5.4'
		jdk 'java'
	}
    stages {
	    stage('Initialize') {
            steps {
                bat '''
				    echo "PATH=$PATH
					echo "M2_HOME=%M2_HOME%
				'''
            }
        }
        stage('Build') {
            steps {
                bat 'Declarative_Pipeline & mvm clean install'

            }
		
        }
       
    }
}
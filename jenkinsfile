pipeline {
    agent any

    
	tools {
	    maven 'Maven3.5.4'
		jdk 'java'
	}
    stages {
	    stage('Initialize') {
            steps {
                bat '''
				    echo "PATH=$PATH
					echo "M2_HOME=%M2_HOME%
				'''
            }
        }
        stage('Build') {
            steps {
                bat 'cd Declarative_Pipeline & mvm insatll'

            }
        }
       
    }
}
